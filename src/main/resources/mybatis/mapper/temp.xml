<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cslc.playlot.order.bet.dao.mapper.BAppConvertCodeMapper">
    <resultMap id="BaseResultMap" type="com.cslc.playlot.order.bet.po.BAppConvertCode">
        <id column="CONVERT_TYPE" jdbcType="DECIMAL" property="convertType" />
        <id column="DOMAIN_CODE" jdbcType="VARCHAR" property="domainCode" />
        <id column="KEY" jdbcType="VARCHAR" property="key" />
        <result column="VAL" jdbcType="VARCHAR" property="val" />
        <result column="DEL_FLAG" jdbcType="DECIMAL" property="delFlag" />
    </resultMap>
    <sql id="Base_Column_List">
        CONVERT_TYPE, DOMAIN_CODE, KEY, VAL, DEL_FLAG
    </sql>
    <select id="select" parameterType="ConditionFields" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from B_APP_CONVERT_CODE
    </select>
    <select id="get" parameterType="com.cslc.playlot.order.bet.po.BAppConvertCodeKey"
            resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from B_APP_CONVERT_CODE
        where CONVERT_TYPE =
        #{convertType,jdbcType=DECIMAL}
        and DOMAIN_CODE =
        #{domainCode,jdbcType=VARCHAR}
        and KEY = #{key,jdbcType=VARCHAR}
    </select>
</mapper>